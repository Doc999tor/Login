{
    "name": "login",
    "version": "1.0.0",
    "main": "main.js",
    "description": "",
    "license": "ISC",
    "author": "kirus",
    "scripts": {
        "webdriver": "webdriver-manager start",
        "start": "webpack-dev-server --port 8080 --hot --host 0.0.0.0 --module-bind 'css=style!css' --mode development",
        "build-p": "webpack -p --progress --env production-p",
        "test": "jest --no-cache",
        "test-watch": "jest --no-cache --watch",
        "build": "webpack",
        "watch": "static"
    },
    "babel": {
        "presets": [
            [
                "es2015",
                {
                    "modules": false
                }
            ],
            "react"
        ],
        "env": {
            "test": {
                "presets": [
                    "es2015",
                    "stage-0",
                    "react"
                ],
                "plugins": [
                    "transform-es2015-modules-commonjs",
                    "dynamic-import-node"
                ]
            }
        }
    },
    "jest": {
        "transform": {
            "^.+\\.jsx$": "babel-jest",
            "^.+\\.js$": "babel-jest"
        },
        "unmockedModulePathPatterns": [
            "node_modules/react/",
            "node_modules/enzyme/"
        ]
    },
    "devDependencies": {
        "babel-core": "^6.24.1",
        "babel-eslint": "^7.1.1",
        "babel-jest": "^21.2.0",
        "babel-loader": "^7.0.0",
        "babel-plugin-dynamic-import-node": "^1.2.0",
        "babel-plugin-transform-es2015-destructuring": "^6.23.0",
        "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
        "babel-plugin-transform-object-rest-spread": "^6.26.0",
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-polyfill": "^6.23.0",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-react": "^6.24.1",
        "babel-preset-stage-0": "^6.24.1",
        "clean-webpack-plugin": "^0.1.18",
        "css-hot-loader": "^1.3.0",
        "css-loader": "^0.28.4",
        "dynamic-cdn-webpack-plugin": "^4.0.0-rc.1",
        "eslint": "^3.19.0",
        "eslint-config-standard": "^6.2.1",
        "eslint-config-standard-react": "^4.2.0",
        "eslint-plugin-promise": "^3.4.0",
        "eslint-plugin-react": "^7.0.1",
        "eslint-plugin-standard": "^2.0.1",
        "extract-text-webpack-plugin": "^4.0.0-beta.0",
        "html-webpack-alter-asset-plugin": "^1.0.0",
        "html-webpack-include-assets-plugin": "^1.0.7",
        "html-webpack-plugin": "^3.2.0",
        "image-webpack-loader": "^3.4.2",
        "less": "^3.0.1",
        "less-loader": "^4.1.0",
        "module-to-cdn": "^3.1.1",
        "npm": "^5.6.0",
        "rename-output-webpack-plugin": "^1.0.1",
        "script-ext-html-webpack-plugin": "^1.8.8",
        "style-loader": "^0.18.1",
        "stylint": "^1.5.9",
        "url-loader": "^0.5.8",
        "webpack": "^4.6.0",
        "copy-webpack-plugin": "^4.6.0",
        "webpack-bundle-analyzer": "^2.9.1",
        "webpack-cli": "^3.2.0",
        "webpack-concat-plugin": "^3.0.0",
        "webpack-dev-server": "^3.1.14"
    },
    "dependencies": {
        "react": "^16.4.1",
        "react-dom": "^16.4.1",
        "react-router-dom": "^4.3.1"
    }
}
